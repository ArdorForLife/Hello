package a20_8_5;

public class MainTest {

	public static void main(String[] args) {
		Circle c=new Circle();
		c.setR(5);
		c.area();
		c.show();
		
		Rectangle r=new Rectangle();
		r.setW(10);
		r.setH(5);
		r.area();
		r.show();
		
		Triangle t=new Triangle();
		t.setW(10);
		t.setH(5);
		t.area();
		t.show();
	}

}
