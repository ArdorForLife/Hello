package test3;

public class MaxTest {

		public static int max(int x, int y) {
			int ma;			//큰값 저장
			if(x>y) ma=x;
			else ma=y;
			return ma;
		}
		
		public static void main(String[] args) {
			String num1=args[0]; //문자열 
			String num2=args[1]; //문자열
			
			int res=max(n1, n2);
			System.out.println("최대값="+res);
		}
}
