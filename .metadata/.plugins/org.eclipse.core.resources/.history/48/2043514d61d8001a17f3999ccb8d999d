import java.util.Scanner;

public class A00012_20_08_07_1330 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		
		if(a>b) System.out.println(">");
		if(a<b) System.out.println("<");
		else if(a==b) System.out.println("==");
		
		
	}

}
